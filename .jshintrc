{
  // Global Variables
  "globals": {
  },
  
  // Enforcing Options
  "bitwise":        true,
  "camelcase":      true,
  "curly":          true,
  "eqeqeq":         true,
  "es3":            true,
  "freeze":         true,
  "immed":          true,
  "indent":         2,
  "latedef":        true,
  "maxdepth":       4,
  "maxstatements":  35,
  "maxcomplexity":  11,
  "newcap":         true,
  "noarg":          true,
  "noempty":        true,
  "nonew":          true,
  "quotmark":       "single",
  "undef":          true,
  "unused":         true,
  "strict":         true,
  
  // Environments
  "browser":        false,
  "node":           true,
  "jquery":         true
}